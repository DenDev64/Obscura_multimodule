<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:bind="http://schemas.android.com/apk/res-auto">

    <data class="TabsThemesDataBinding">
        <variable name="bindingModel"
                  type="com.obscura.llc.obscuraproject.presentation.fragments.main_tabs.tabs_themes.TabsThemesBindingModel"/>
    </data>
    <FrameLayout android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 tools:context=".presentation.fragments.main_tabs.tabs_themes.TabsThemesFragment">
        <com.obscura.llc.obscuraproject.presentation.widget.pageview.PagesView
                android:id="@+id/pages_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                bind:tabs="@{bindingModel.tabs}"/>
    </FrameLayout>
</layout>